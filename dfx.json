{
  "canisters": {
    "iris_backend": {
      "candid": "src/iris_backend/iris_backend.did",
      "package": "iris_backend",
      "type": "rust",
      "init_arg": "(record { bitcoin_network = variant { testnet }; ecdsa_key_name = \"test_key_1\" })"
    },
    "iris_frontend": {
      "dependencies": [
        "iris_backend"
      ],
      "source": [
        "src/iris_frontend/assets"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}