{
  "canisters": {
    "iris_backend": {
      "candid": "src/iris_backend/iris_backend.did",
      "package": "iris_backend",
      "type": "rust"
    },
    "iris_frontend": {
      "dependencies": [
        "iris_backend"
      ],
      "source": [
        "src/iris_frontend/dist"
      ],
      "type": "assets"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    }
  },
  "output_env_file": ".env",
  "version": 1
}
